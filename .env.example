# Environment
NODE_ENV=development

# Database (use DATABASE_URL para Docker)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/jurix
DB_HOST=localhost
DB_PORT=5432
DB_NAME=jurix
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false

# JWT
JWT_SECRET=your-super-secret-jwt-key-min-32-chars
JWT_EXPIRES_IN=7d

# AWS
AWS_REGION=us-east-1
S3_BUCKET=jurix-uploads

# App
CORS_ORIGINS=*
